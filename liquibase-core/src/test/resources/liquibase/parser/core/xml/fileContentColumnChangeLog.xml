<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
 xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.1.xsd">
    
    <changeSet id="1" author="njansen">
        <update tableName="text">
            <column name="content" type="text">
                <fileValue path="liquibase/change/core/text.txt" encoding="UTF-8" />
            </column>
            <where>
                id=1
            </where>
        </update>
    </changeSet>
</databaseChangeLog>
